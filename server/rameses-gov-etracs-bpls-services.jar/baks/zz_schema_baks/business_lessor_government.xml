<!--
DROP VIEW IF EXISTS business_lessor_government;
CREATE VIEW business_lessor_government AS 
SELECT * FROM business_lessor WHERE government = 1
-->
<schema>
	<element tablename="business_lessor_government">
		<field name="objid" primary="true"/>
		<field name="government" required="true" defaultValue="1" type="integer" visible="false"/>
		<field name="bldgno" caption="Bldg No"/>
		<field name="bldgname" required="true" searchable="true" indexed="true" caption="Bldg Name"/>
		<field name="street"  required="true" caption="Street"/>
		<field name="subdivision" indexed="true" caption="Subdivision"/>
		<field name="barangay_objid" visible="false"/>
		<field name="barangay_name" indexed="true" caption="Barangay"/>
		<field name="pin" caption="PIN"/>
	</element>
</schema>			

