<schema adapter="bplsdb">
	
	<element tablename="business_application_lob">
		<field name="objid" primary="true" prefix="BAPPLOB"/>
		<field name="appid" />
		
		<field name="lobid" required="true"/>
		<field name="assessmenttype" required="true"/>
		<field name="particulars" />

		<complex name="lob" ref="lob" jointype="many-to-one" includefields="objid,name,classificationid,psicid">
			<key field="lobid" target="objid"/>
		</complex>	
	</element>

</schema>
